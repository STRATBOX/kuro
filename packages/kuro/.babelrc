{
  "plugins": [
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    [
      "module-resolver",
      {
        "alias": {
          "react-native": "react-native-web"
        },
        "extensions": ["web.js", ".js", "web.jsx", ".jsx"]
      }
    ]
  ],
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      }
    }],
    "react", "stage-2", "flow"
  ]
}
